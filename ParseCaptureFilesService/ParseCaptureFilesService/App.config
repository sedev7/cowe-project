<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
    <appSettings>
      <add key="INPUT_FILE_DIR" value ="C:\temp\CaptureFiles"/>
      <add key="LOG_FILE_DIR" value ="C:\temp\ParseCaptureServiceLog"/>
      <add key="PARSED_FILES_DIR" value ="C:\temp\ParsedCaptureFiles"/>
      <add key="ERROR_FILES_DIR" value ="C:\temp\ParsedCaptureFileErrors"/>
      <!-- Include a log for each file parsed? [true|false]-->
      <add key="PARSED_FILES_LOG" value ="true"/>
      <add key="DB_CONN_STR" value ="Integrated Security=SSPI; Initial Catalog=Packets; Data Source=Neptune"/>
      <!-- Log levels: info, warn, error, debug -->
      <add key="LOG_LEVEL" value ="debug"/>
    </appSettings>
</configuration>